<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="0dp">

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/contendor_fragmento"
        android:name="com.pepinho.nba.EquipoFragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:context="com.pepinho.nba.MainActivity" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/contendor_fragmento2"
        android:name="com.pepinho.nba.EquipoFragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/contendor_fragmento"
        tools:context="com.pepinho.nba.MainActivity" />

</androidx.constraintlayout.widget.ConstraintLayout>

